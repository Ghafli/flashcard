{
  "name": "flashcard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ghafli.github.io/flashcard",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "predeploy": "npm run build",
    "deploy": "touch out/.nojekyll && git add out/ && git commit -m \"Deploy to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "clean": "rimraf .next out",
    "prebuild": "npm run clean",
    "postbuild": "next-sitemap",
    "migrate:up": "ts-node scripts/migrate.ts up",
    "migrate:down": "ts-node scripts/migrate.ts down",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@firebase/analytics": "^0.10.10",
    "@firebase/app": "^0.10.16",
    "@firebase/auth": "^1.8.1",
    "@firebase/database": "^1.0.10",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.19",
    "@mui/lab": "^5.0.0-alpha.155",
    "@mui/material": "^5.14.20",
    "@next-auth/mongodb-adapter": "^1.1.3",
    "@types/chart.js": "^2.9.41",
    "@types/react-router-dom": "^5.3.3",
    "bcryptjs": "^2.4.3",
